<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sridhar.deepak.digitdetection">

    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/numbers"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Deep">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.Aboutus"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Dialog">
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.ABOUT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.Pref"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.PREF" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.CatsvsDogs"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.CATSVSDOGS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.DigitClassifier"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.DIGITCLASSIFIER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.TensorFlowDemo"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.TENSORFLOWDEMO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sridhar.deepak.digitdetection.HappyFaceDetector"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.sridhar.deepak.digitdetection.HAPPYFACEDETECTOR" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>